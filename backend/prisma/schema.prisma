datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URI")
}

//Needed for client generation
generator client {
  provider = "prisma-client-js"
}

model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  username    String 
  rankedScore Int  @default(3000)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}